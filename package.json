{
	"name": "@lookwe/lui",
	"description": "Lookwe UI web components",
	"homepage": "https://github.com/Lookwe69/lui",
	"bugs": {
		"url": "https://github.com/Lookwe69/lui/issues"
	},
	"keywords": [
		"lui",
		"web-components",
		"lit-element",
		"typescript",
		"lit"
	],
	"author": "Lookwe",
	"version": "0.0.0-development",
	"type": "module",
	"scripts": {
		"build": "rollup --config rollup-dist.config.js",
		"build:watch": "npm run build -- --watch",
		"build:prepack": "rollup --config rollup-prepack.config.js",
		"clean": "rm -rf dist && rm -rf coverage && rm -rf src/react && rm -rf components && rm -rf internal && rm -rf react && rm -f custom-elements.json && rm -f vscode.html-custom-data.json && rm -f web-types.json",
		"lint": "npm run lint:lit-analyzer && npm run lint:eslint && npm run lint:ts",
		"lint:eslint": "eslint 'src/**/*.ts'",
		"lint:lit-analyzer": "lit-analyzer",
		"lint:ts": "tsc --noEmit",
		"format": "prettier --check --ignore-path .gitignore --ignore-path .prettierignore .",
		"format:fix": "npm run format -- --write && npm run format -- --write --plugin=@homer0/prettier-plugin-jsdoc",
		"serve": "wds --watch",
		"serve:prod": "MODE=prod npm run serve",
		"test": "npm run build && wtr && MODE=prod wtr",
		"test:dev": "npm run build && wtr",
		"test:dev:watch": "npm run build && wtr --watch",
		"test:prod": "MODE=prod wtr",
		"test:prod:watch": "npm run build && MODE=prod wtr --watch",
		"test:coverage": "npm run test -- --coverage",
		"create": "plop --plopfile scripts/plop/create.js",
		"create-variant": "plop --plopfile scripts/plop/create-variant.js",
		"make-metadata": "node ./scripts/make-metadata.js --outdir dist",
		"make-metadata:prepack": "node ./scripts/make-metadata.js --outdir ./",
		"make-react": "node ./scripts/make-react.js --customElements dist/custom-elements.json",
		"make-react:prepack": "node ./scripts/make-react.js --customElements ./custom-elements.json",
		"prepare-pack": "npm run clean && npm run make-metadata:prepack && npm run make-react:prepack && npm run build:prepack",
		"prepack": "npm run prepare-pack",
		"semantic-release": "semantic-release"
	},
	"files": [
		"components",
		"internal",
		"react",
		"tokens",
		"custom-elements.json",
		"vscode.html-custom-data.json",
		"web-types.json"
	],
	"main": "components/all.js",
	"module": "components/all.js",
	"types": "components/all.d.ts",
	"customElements": "custom-elements.json",
	"web-types": "web-types.json",
	"exports": {
		"./custom-elements.json": "./custom-elements.json",
		"./components/*": "./components/*",
		"./components/*/internal/*": null,
		"./react/*": "./react/*"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@lit-labs/observers": "^2.0.6",
		"@lit/react": "^1.0.8",
		"@lookwe/lit-controllers": "^1.1.4",
		"@lookwe/lit-mixins": "^1.0.1",
		"@lookwe/omit-undefined": "^1.1.0",
		"@lookwe/utils": "^1.1.2",
		"lit": "^3.3.1",
		"tslib": "^2.8.1"
	},
	"devDependencies": {
		"@custom-elements-manifest/analyzer": "^0.10.4",
		"@homer0/prettier-plugin-jsdoc": "^10.0.0",
		"@ianvs/prettier-plugin-sort-imports": "^4.5.1",
		"@literals/rollup-plugin-html-css-minifier": "^3.0.1",
		"@open-wc/testing": "^4.0.0",
		"@rollup/plugin-node-resolve": "^16.0.1",
		"@rollup/plugin-replace": "^6.0.2",
		"@rollup/plugin-typescript": "^12.1.4",
		"@tsconfig/strictest": "^2.0.5",
		"@types/mocha": "^10.0.10",
		"@web/dev-server": "^0.4.6",
		"@web/dev-server-rollup": "^0.6.4",
		"@web/test-runner": "^0.20.2",
		"@web/test-runner-commands": "^0.9.0",
		"@web/test-runner-playwright": "^0.11.1",
		"custom-element-jet-brains-integration": "^1.7.0",
		"custom-element-vs-code-integration": "^1.5.0",
		"eslint": "^9.32.0",
		"globals": "^16.3.0",
		"lit-analyzer": "^2.0.3",
		"plop": "^4.0.1",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"prettier-plugin-css-order": "^2.1.2",
		"rollup": "^4.46.2",
		"rollup-plugin-multi-input": "^1.5.0",
		"rollup-plugin-postcss": "^4.0.2",
		"rollup-plugin-postcss-lit": "^2.2.0",
		"sass": "^1.89.2",
		"semantic-release": "^24.2.7",
		"sinon": "^21.0.0",
		"ts-lit-plugin": "^2.0.2",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.38.0"
	}
}
